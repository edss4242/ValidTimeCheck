{
  "manifest_version": 2,
  "name": "ValidTimeCheck",
  "description": "Verifies if a time is valid",
  "version": "1.0",
  "icons": {
    "48": "icon.png"
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ],
  "web_accessible_resources": [
    "index.html"
  ]
}
